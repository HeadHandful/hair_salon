{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "prettier/prettier": "error",
      "no-unused-vars": "error",
      "no-console": "off",
      "react/react-in-jsx-scope": "off",
      "import/prefer-default-export": "off",
      "react/function-component-definition": "off"
    },
    "env": {
        "browser": true,
        "node": true
      }
  }